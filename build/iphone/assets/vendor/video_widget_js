exports.create=function(e){if(0!=Ti.Network.online&&e.mp4){var t=e.mp4,r=Ti.UI.createWindow({backgroundColor:"white",orientationModes:[Ti.UI.LANDSCAPE_RIGHT,Ti.UI.LANDSCAPE_LEFT]});r.add(Ti.UI.createImageView({image:e.poster}));var i=Ti.Media.createVideoPlayer({autoplay:!0,fullscreen:!0,backgroundColor:"#333",url:t,mediaControlStyle:Ti.Media.VIDEO_CONTROL_DEFAULT,scalingMode:Ti.Media.VIDEO_MODE_FILL});i.addEventListener("playbackstate",function(e){console.log(e.playbackState)}),i.addEventListener("complete",function(e){0==e.reason&&r.close()}),i.addEventListener("fullscreen",function(e){0==e.entering&&r.close()})}};