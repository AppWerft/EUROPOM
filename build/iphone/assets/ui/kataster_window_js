const START=.01;exports.create=function(){var e=Ti.UI.createWindow({fullscreen:!0,title:"Baumkataster Streuobstbestände"}),t=Ti.UI.createScrollView({top:0,left:"10dp",right:"10dp",height:Ti.UI.FILL,width:Ti.UI.FILL,contentHeight:Ti.UI.SIZE,scrollType:"vertical",layout:"vertical"});e.add(t);var i=Ti.UI.createLabel({text:"Bestandsaufnahme von Streuobstwiesen/ Obstgärten",top:"10dp",width:Ti.UI.FILL,backgroundColor:"white",textAlign:"center",color:"#343",font:{fontSize:"22dp",fontWeight:"bold",fontFamily:"Vollkorn-Regular"}});t.add(i);var o=Ti.UI.createSwitch({titleOff:" Die Daten sind nur für den internen Gebrauch des PV zu verwenden",titleOn:"Die Daten dürfen veröffentlicht werden.",value:!0,top:"10dp",width:Ti.UI.FILL,height:120});t.add(o);var r=Ti.UI.createView({top:"10dp",height:"70dp"});r.text=Ti.UI.createLabel({left:0,width:"45%"}),r.button=Ti.UI.createButton({title:"Standortkarte",right:0,width:"45%"}),r.button.addEventListener("click",function(){var e=require("ui/gmap.widget").create();e.open()}),r.add(r.text),r.add(r.button),t.add(r);for(var n=Ti.App.PommesModel.getAllSorten(),a=Ti.UI.createPicker({top:"10dp",right:"10dp",useSpinner:!1}),l=[],d=0;n.length>d;d++)l.push(Ti.UI.createPickerRow({title:n[d]}));a.add(l),a.selectionIndicator=!0,t.add(a);var s=Ti.UI.createTextField({borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,color:"#336699",top:"10dp",width:Ti.UI.FILL,height:"50dp",hintText:"Besitzer"});t.add(s);var p=Ti.UI.createTextField({borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,color:"#336699",top:"10dp",width:Ti.UI.FILL,height:"50dp",hintText:"Betreiber"});t.add(p);var c=Ti.UI.createTextField({borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,color:"#336699",top:"10dp",width:Ti.UI.FILL,height:"50dp",hintText:"Ansprechpartner"});t.add(c);var u=Ti.UI.createTextField({borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,color:"#336699",top:"10dp",width:Ti.UI.FILL,height:"50dp",hintText:"Pomologe"});t.add(u);var h=Ti.UI.createTextArea({borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,color:"#336699",top:"10dp",width:Ti.UI.FILL,height:"300dp",hintText:"Bemerkungen"});return t.add(h),e};