Array.prototype.isArray=!0,exports.create=function(){var e=Ti.UI.createWindow({backgroundImage:"/assets/bg.png",barColor:"#040",exitOnClose:!0,title:"Apfel-Nachrichten"});e.listView=Ti.UI.createListView({templates:{feed:require("ui/TEMPLATES").feedrow},defaultItemTemplate:"feed"}),e.add(e.listView),e.listView.addEventListener("itemclick",function(t){Ti.Android?require("ui/web.window").create(t.itemId).open():e.tab.open(require("ui/web.window").create(t.itemId))});var t=function(){require("vendor/cachedxhr").get({url:"http://pomologen-verein.de/startseite/rss.xml",onload:function(t){console.log(t);var i=[];if(!t.channel||!t.channel.description)return console.log("Warning: no valide RSS"),void 0;e.title=t.channel.description;var o=t.channel.item&&t.channel.item.isArray?t.channel.item:[],n=[];if(o.isArray)for(var r=0;o.length>r;r++){var s=o[r].description.replace(/\.\.\./," …");console.log(s),n.push({title:{text:o[r].title},text:{text:s},properties:{itemId:JSON.stringify(o[r]),accessoryType:Ti.UI.LIST_ACCESSORY_TYPE_DISCLOSURE}})}console.log(n),i.push(Ti.UI.createListSection({items:n})),e.listView.setSections(i)}})};return e.addEventListener("focus",t),console.log("================="),e};