exports.create=function(e){var t=JSON.parse(e),i=Ti.UI.createWindow({backgroundColor:"white",title:t.title,fullscreen:!1});if(Ti.Android){var o=Ti.UI.createScrollView({height:Ti.UI.FILL,width:Ti.UI.FILL,contentHeight:Ti.UI.SIZE,scrollType:"vertical"});i.add(o);var r=Ti.UI.createLabel({font:{fontSize:"18dp",fontWeight:"bold",fontFamily:"Vollkorn-Regular"},html:t["content:encoded"],color:"#464",width:Ti.UI.FILL,top:"10dp",left:"10dp",right:"10dp",bottom:"10dp",height:Ti.UI.FILL});r.addEventListener("load",function(){i.backgroundColor="white"}),o.add(r)}else{var o=Ti.UI.createWebView({height:Ti.UI.FILL,width:Ti.UI.FILL,html:t["content:encoded"]});i.add(o)}return i};