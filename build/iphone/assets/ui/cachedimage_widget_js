exports.create=function(e){var t="cache4image",i=e.url,o=e.view,r=e.onload,n=i.split("/");n=n[n.length-1];var s=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,t,n);if(s.exists())o&&(o.image=s.nativePath),r&&r(s.nativePath),s=null;else{var a=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,t);a.exists()||a.createDirectory();var p=Ti.Network.createHTTPClient();p.onload=function(){200==p.status&&(s.write(p.responseData),o&&(o.image=s.nativePath),r&&r(s.nativePath),s=null)},p.open("GET",i),p.send()}};